<%@ Master Language="C#" AutoEventWireup="true" CodeBehind="Customer.master.cs" Inherits="KpopZtation.View.Master.Customer" %>
<!DOCTYPE html>
<html>
<head runat="server">
    <title></title>
    <asp:ContentPlaceHolder ID="head" runat="server"></asp:ContentPlaceHolder>
    <link rel="stylesheet" href="../StyleCSS/authorizedNavBarStyle.css" />
</head>
<body>
    <form id="form1" runat="server">
        <div class="containerNav">
            <div class="navbar">
                <div>
                    <asp:Label ID="kpopZtation" runat="server" Text="KpopZtation" CssClass="navbar-logo"></asp:Label>
                </div>
                <div class="navbar-menu">
                    <asp:HyperLink runat="server" NavigateUrl="~/View/home.aspx" Text="Home"></asp:HyperLink>
                    <asp:HyperLink runat="server" NavigateUrl="~/View/cart.aspx" Text="Cart"></asp:HyperLink>
                    <asp:HyperLink runat="server" NavigateUrl="~/View/transaction.aspx" Text="Transaction"></asp:HyperLink>
                    <asp:HyperLink runat="server" NavigateUrl="~/View/update_profile.aspx" Text="Update Profile"></asp:HyperLink>
                </div>
                <div>
                    <asp:LinkButton ID="logOut" runat="server" OnClick="logOut_Click" CssClass="logout-button" style="text-decoration: none !important;">Logout</asp:LinkButton>
                </div>
            </div>
            <div class="content">
                <asp:ContentPlaceHolder ID="ContentPlaceHolder1" runat="server"></asp:ContentPlaceHolder>
            </div>
            <div class="footer">
                &copy; 2023 - KpopZtation
            </div>
        </div>
    </form>
</body>
</html>
